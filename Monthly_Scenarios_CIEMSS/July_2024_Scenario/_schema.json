                     },
                     "size": {
                        "value": 35
                     },
                     "strokeWidth": {
                        "value": 2
                     },
                     "fill": {
                        "scale": "color",
                        "field": "trajectory"
                     },
                     "stroke": {
                        "scale": "color",
                        "field": "trajectory"
                     },
                     "fillOpacity": [
                        {
                           "test": "!length(data('selected')) || indata('selected', 'value', datum.trajectory)",
                           "value": 0.5
                        },
                        {
                           "value": 0.15
                        }
                     ],
                     "strokeOpacity": [
                        {
                           "test": "!length(data('selected')) || indata('selected', 'value', datum.trajectory)",
                           "value": 0.75
                        },
                        {
                           "value": 0.15
                        }
                     ],
                     "tooltip": {
                        "field": "value"
                     }
                  },
                  "hover": {
                     "fillOpacity": {
                        "value": 0.9
                     }
                  }
               }
            },
            {
               "type": "line",
               "name": "_points_line",
               "from": {
                  "data": "points_split"
               },
               "zindex": 3,
               "encode": {
                  "update": {
                     "x": {
                        "scale": "xscale",
                        "field": "timepoint"
                     },
                     "y": {
                        "scale": "yscale",
                        "field": "value"
                     },
                     "strokeWidth": {
                        "value": 2
                     },
                     "stroke": {
                        "scale": "color",
                        "field": "trajectory"
                     },
                     "interpolate": {
                        "value": "linear"
                     },
                     "strokeDash": {
                        "value": [
                           5,
                           3
                        ]
                     },
                     "strokeOpacity": [
                        {
                           "test": "!length(data('selected')) || indata('selected', 'value', datum.trajectory)",
                           "value": 0.3
                        },
                        {
                           "value": 0.1
                        }
                     ]
                  },
                  "hover": {
                     "fillOpacity": {
                        "value": 0.9
                     }
                  }
               }
            }
         ]
      },
      {
         "type": "group",
         "zindex": 2,
