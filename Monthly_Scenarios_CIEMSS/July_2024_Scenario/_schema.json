                        "value": 0
                     },
                     "x": {
                        "signal": "width"
                     },
                     "opacity": {
                        "value": 1
                     }
                  }
               }
            },
            {
               "type": "text",
               "name": "_marker_h flags",
               "from": {
                  "data": "_marker_h text"
               },
               "encode": {
                  "enter": {
                     "text": {
                        "field": "datum.label"
                     },
                     "x": {
                        "signal": "width+20"
                     },
                     "fontSize": {
                        "value": 8
                     }
                  }
               },
               "transform": [
                  {
                     "type": "label",
                     "lineAnchor": "start",
                     "as": [
                        "_x",
                        "y",
                        "opacity",
                        "align",
                        "baseline"
                     ],
                     "offset": [
                        1
                     ],
                     "size": {
                        "signal": "[width, height]"
                     }
                  }
               ]
            }
         ]
      }
