                     "fillOpacity": {
                        "value": 0.9
                     }
                  }
               }
            }
         ]
      },
      {
         "type": "group",
         "name": "_markers_v",
         "marks": [
            {
               "type": "rule",
               "name": "_marker_v text",
               "from": {
                  "data": "markers_v"
               },
               "encode": {
                  "enter": {
                     "stroke": {
                        "value": "lightgray"
                     }
                  },
                  "update": {
                     "x": {
                        "scale": "xscale",
                        "field": "axis_value"
                     },
                     "y2": {
                        "value": 0
                     },
                     "y": {
                        "signal": "height"
                     },
                     "opacity": {
                        "value": 1
                     }
                  }
               }
            },
            {
               "type": "text",
               "name": "_marker_v flags",
               "from": {
                  "data": "_marker_v text"
               },
               "encode": {
                  "enter": {
                     "text": {
                        "field": "datum.label"
                     },
                     "y": {
                        "value": 0
                     },
                     "fontSize": {
                        "value": 8
                     }
                  }
               },
               "transform": [
                  {
                     "type": "label",
                     "lineAnchor": "start",
                     "as": [
                        "x",
                        "_y",
                        "opacity",
                        "align",
                        "baseline"
                     ],
                     "offset": [
                        1
                     ],
                     "size": {
                        "signal": "[width, height]"
                     }
                  }
               ]
            }
         ]
      },
      {
         "type": "group",
         "name": "_markers_h",
         "marks": [
            {
               "type": "rule",
               "name": "_marker_h text",
               "from": {
