            "facet": {
               "name": "trajectory_split",
               "data": "distributions",
               "groupby": "trajectory"
            }
         },
         "marks": [
            {
               "type": "area",
               "name": "area",
               "interactive": true,
               "from": {
                  "data": "trajectory_split"
