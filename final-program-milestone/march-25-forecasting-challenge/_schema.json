      {
         "name": "markers_v",
         "values": []
      },
      {
         "name": "markers_h",
         "values": []
      },
      {
         "name": "selected",
         "on": [
            {
               "trigger": "clear",
               "remove": true
            },
            {
               "trigger": "!shift",
               "remove": true
            },
            {
               "trigger": "!shift && clicked",
               "insert": "clicked"
            },
            {
               "trigger": "shift && clicked",
               "toggle": "clicked"
            }
         ]
      },
      {
         "name": "selectedData",
         "on": [
            {
               "trigger": "clearData",
               "remove": true
            },
            {
               "trigger": "clickedData",
               "toggle": "clickedData"
            }
         ]
      }
